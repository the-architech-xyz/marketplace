{
  "id": "connectors/ui/tamagui-expo",
  "name": "Tamagui + Expo Connector",
  "description": "Tamagui UI integration for Expo/React Native applications",
  "type": "connector",
  "version": "1.0.0",
  "role": "framework-wiring-connector",
  "pattern": "infrastructure",
  "connects": [
    "ui/tamagui",
    "framework/expo"
  ],
  "provides": [
    "tamagui-expo",
    "tamagui-react-native",
    "tamagui-provider",
    "safe-area-context"
  ],
  "requires": [
    "framework/expo",
    "ui/tamagui"
  ],
  "parameters": {
    "safeArea": {
      "type": "boolean",
      "default": true,
      "description": "Enable SafeAreaProvider wrapper"
    }
  },
  "prerequisites": [
    "adapters/framework/expo",
    "adapters/ui/tamagui"
  ]
}
