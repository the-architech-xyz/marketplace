{
  "id": "connectors/email/resend-nextjs",
  "name": "Resend + Next.js Connector",
  "description": "Resend email service integration for Next.js applications",
  "type": "connector",
  "version": "1.0.0",
  "role": "framework-wiring-connector",
  "pattern": "infrastructure",
  "connects": [
    "resend",
    "nextjs"
  ],
  "provides": [
    "email-sending",
    "email-api-routes"
  ],
  "requires": [
    "framework/nextjs",
    "email/resend"
  ],
  "capabilities": {
    "email": {
      "enabled": true,
      "features": {
        "transactional": true,
        "templates": false,
        "campaigns": false,
        "analytics": false
      }
    }
  },
  "parameters": {
    "apiKey": {
      "type": "string",
      "required": true,
      "description": "Resend API key"
    }
  }
}
