    },
    {
      "key": "apps.mobile.root",
      "description": "Mobile app root directory",
      "required": false,
      "structure": "both",
      "computed": true,
      "group": "mobile-app"
    },
    {
      "key": "apps.mobile.src",
      "description": "Mobile app source directory",
      "required": false,
      "structure": "both",
      "computed": true,
      "group": "mobile-app"
    },
    {
      "key": "apps.mobile.components",
      "description": "Mobile app React components directory",
      "required": false,
      "structure": "both",
      "computed": true,
      "group": "mobile-app"
    },
    {
      "key": "apps.all.root",
      "description": "All apps root directory (semantic category - resolves to all apps)",
      "required": false,
      "structure": "monorepo",
      "computed": true,
      "semantic": true,
      "resolveToApps": "all",
      "group": "semantic-apps"
    },
    {
      "key": "apps.all.src",
      "description": "All apps source directory (semantic category)",
      "required": false,
      "structure": "monorepo",
      "computed": true,
      "semantic": true,
      "resolveToApps": "all",
      "group": "semantic-apps"
    },
    {
      "key": "apps.all.components",
      "description": "All apps components directory (semantic category)",
      "required": false,
      "structure": "monorepo",
      "computed": true,
      "semantic": true,
      "resolveToApps": "all",
      "group": "semantic-apps"
    },
    {
      "key": "apps.all.app",
      "description": "All apps app directory (semantic category - Next.js only)",
      "required": false,
      "structure": "monorepo",
      "computed": true,
      "semantic": true,
      "resolveToApps": "all",
      "group": "semantic-apps"
    },
    {
      "key": "apps.all.middleware",
      "description": "All apps middleware directory (semantic category - Next.js only)",
      "required": false,
      "structure": "monorepo",
      "computed": true,
      "semantic": true,
      "resolveToApps": "all",
      "group": "semantic-apps"
    },
    {
      "key": "apps.frontend.root",
      "description": "Frontend apps root directory (semantic category - resolves to web + mobile)",
      "required": false,
      "structure": "monorepo",
      "computed": true,
      "semantic": true,
      "resolveToApps": ["web", "mobile"],
      "group": "semantic-apps"
    },
    {
      "key": "apps.frontend.src",
      "description": "Frontend apps source directory (semantic category)",
      "required": false,
      "structure": "monorepo",
      "computed": true,
      "semantic": true,
      "resolveToApps": ["web", "mobile"],
      "group": "semantic-apps"
    },
    {
      "key": "apps.frontend.components",
      "description": "Frontend apps components directory (semantic category)",
      "required": false,
      "structure": "monorepo",
      "computed": true,
      "semantic": true,
      "resolveToApps": ["web", "mobile"],
      "group": "semantic-apps"
    },
    {
      "key": "apps.frontend.app",
      "description": "Frontend apps app directory (semantic category - Next.js only)",
      "required": false,
      "structure": "monorepo",
      "computed": true,
      "semantic": true,
      "resolveToApps": ["web", "mobile"],
      "group": "semantic-apps"
    },
    {
      "key": "apps.frontend.middleware",
      "description": "Frontend apps middleware directory (semantic category - Next.js only)",
      "required": false,
      "structure": "monorepo",
      "computed": true,
      "semantic": true,
      "resolveToApps": ["web", "mobile"],
      "group": "semantic-apps"
    },
    {
      "key": "apps.backend.root",
      "description": "Backend apps root directory (semantic category - resolves to api)",
      "required": false,
      "structure": "monorepo",
      "computed": true,
      "semantic": true,
      "resolveToApps": ["api"],
      "group": "semantic-apps"
    },
    {
      "key": "apps.backend.src",
      "description": "Backend apps source directory (semantic category)",
      "required": false,
      "structure": "monorepo",
      "computed": true,
      "semantic": true,
      "resolveToApps": ["api"],
      "group": "semantic-apps"
    },
    {
      "key": "apps.backend.routes",
      "description": "Backend apps routes directory (semantic category)",
      "required": false,
      "structure": "monorepo",
      "computed": true,
      "semantic": true,
      "resolveToApps": ["api"],
      "group": "semantic-apps"
    },
    {
      "key": "apps.backend.api",
      "description": "Backend API routes directory (semantic category - resolves to apps.api.routes or apps.web.app/api based on project structure)",
      "required": false,
      "structure": "both",
      "computed": true,
      "semantic": true,
      "resolveToApps": ["api", "web"],
      "resolutionStrategy": {
        "priority": ["api", "web"],
        "fallback": "web"
      },
      "group": "semantic-backend"
    },
    {
      "key": "apps.backend.server",
      "description": "Backend server-side code directory (semantic category - resolves to apps.api.src or apps.web.server based on project structure)",
      "required": false,
      "structure": "both",
      "computed": true,
      "semantic": true,
      "resolveToApps": ["api", "web"],
      "resolutionStrategy": {
        "priority": ["api", "web"],
        "fallback": "web"
      },
      "group": "semantic-backend"
    },
    {
      "key": "packages.shared.root",
      "description": "Shared package root directory",
      "required": false,
      "structure": "both",
      "computed": true,
      "group": "shared-packages"
    },
    {
