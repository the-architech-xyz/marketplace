      "computed": true,
      "group": "shared-packages"
    },
    {
      "key": "packages.shared.src.stores",
      "description": "Shared state stores (Zustand, etc.)",
      "required": false,
      "structure": "both",
      "computed": true,
      "group": "shared-packages"
    },
    {
      "key": "packages.shared.src.types",
      "description": "Shared TypeScript types",
      "required": false,
      "structure": "both",
      "computed": true,
      "group": "shared-packages"
    },
    {
      "key": "packages.shared.src.utils",
      "description": "Shared utility functions",
      "required": false,
      "structure": "both",
      "computed": true,
      "group": "shared-packages"
    },
    {
      "key": "packages.shared.src.scripts",
      "description": "Shared scripts",
      "required": false,
      "structure": "both",
      "computed": true,
      "group": "shared-packages"
    },
    {
      "key": "packages.shared.src.routes",
      "description": "Shared route components (for multi-app routing)",
      "required": false,
      "structure": "both",
      "computed": true,
      "group": "shared-packages"
    },
    {
      "key": "packages.shared.src.jobs",
      "description": "Shared background jobs (Inngest, BullMQ, etc.)",
      "required": false,
      "structure": "both",
      "computed": true,
      "group": "shared-packages"
    },
    {
      "key": "packages.database.root",
      "description": "Database package root directory",
      "required": false,
      "structure": "both",
      "computed": true,
      "group": "shared-packages"
    },
    {
      "key": "packages.database.src",
      "description": "Database package source directory",
      "required": false,
      "structure": "both",
      "computed": true,
      "group": "shared-packages"
    },
    {
      "key": "packages.ui.root",
      "description": "UI package root directory",
      "required": false,
      "structure": "both",
      "computed": true,
      "group": "shared-packages"
    },
    {
      "key": "packages.ui.src",
      "description": "UI package source directory",
      "required": false,
      "structure": "both",
      "computed": true,
      "group": "shared-packages"
    },
    {
      "key": "packages.{packageName}.root",
      "description": "Package root directory (dynamic)",
      "required": true,
      "structure": "monorepo",
      "defaultValue": "packages/{packageName}/",
      "computed": false,
      "variables": ["packageName"],
      "group": "packages"
    },
    {
      "key": "packages.{packageName}.src",
      "description": "Package source directory (dynamic)",
      "required": true,
      "structure": "monorepo",
      "defaultValue": "packages/{packageName}/src/",
      "computed": false,
      "variables": ["packageName"],
      "group": "packages"
    },
    {
      "key": "apps.{appId}.root",
      "description": "App root directory (dynamic)",
      "required": true,
      "structure": "monorepo",
      "defaultValue": "apps/{appId}/",
      "computed": false,
      "variables": ["appId"],
      "group": "apps"
    },
    {
      "key": "apps.{appId}.src",
      "description": "App source directory (dynamic)",
      "required": true,
      "structure": "monorepo",
      "defaultValue": "apps/{appId}/src/",
      "computed": false,
      "variables": ["appId"],
      "group": "apps"
    },
    {
      "key": "apps.{appId}.components",
      "description": "App components directory (framework-specific)",
      "required": false,
      "structure": "monorepo",
      "defaultValue": {
        "nextjs": "apps/{appId}/src/components/",
        "expo": "apps/{appId}/components/"
      },
      "computed": true,
      "variables": ["appId"],
      "group": "apps"
    },
    {
      "key": "apps.{appId}.app",
      "description": "Next.js app directory (App Router) - dynamic",
      "required": false,
      "structure": "monorepo",
      "defaultValue": "apps/{appId}/src/app/",
      "computed": false,
      "variables": ["appId"],
      "group": "apps"
    },
    {
      "key": "ui.framework",
      "description": "Active UI framework identifier (e.g., shadcn, tamagui, tailwind)",
      "required": false,
      "structure": "both",
      "computed": true,
      "group": "ui"
    },
    {
      "key": "ui.marketplace",
      "description": "Resolved UI marketplace path on disk (set by CLI)",
      "required": false,
      "structure": "both",
      "computed": true,
      "group": "ui"
    },
    {
      "key": "ui.path",
      "description": "Legacy UI marketplace path key (backwards compatibility)",
      "required": false,
      "structure": "both",
      "computed": true,
      "deprecated": true,
      "replacement": "ui.marketplace",
      "group": "ui"
    }
  ],
  "groups": {
    "workspace": ["workspace.*"],
    "web-app": ["apps.web.*"],
    "api-app": ["apps.api.*"],
    "semantic-apps": ["apps.frontend.*", "apps.backend.*", "apps.all.*"],
    "semantic-backend": ["apps.backend.api", "apps.backend.server"],
    "shared-packages": ["packages.shared.*", "packages.database.*", "packages.ui.*"],
    "ui": ["ui.*"]
  }
}



