    },
    {
      "key": "workspace.scripts",
      "description": "Workspace scripts directory",
      "required": true,
      "structure": "both",
      "defaultValue": "./scripts/",
      "computed": false,
      "group": "workspace"
    },
    {
      "key": "workspace.docs",
