{
  "id": "shadcn-ui",
  "name": "Shadcn/ui",
  "description": "Beautifully designed components built with Radix UI and Tailwind CSS",
  "category": "ui",
  "version": "1.0.0",
  "blueprint": "blueprint.ts",
  "provides": [
    {
      "name": "shadcn-ui",
      "version": "latest",
      "description": "Shadcn/ui component library"
    },
    {
      "name": "radix-ui",
      "version": "1.0.0",
      "description": "Radix UI primitive components"
    }
  ],
  "prerequisites": {
    "capabilities": [
      {
        "name": "react",
        "version": ">=18.0.0",
        "description": "React library required"
      },
      {
        "name": "tailwind",
        "version": ">=3.0.0",
        "description": "Tailwind CSS required"
      },
      {
        "name": "typescript",
        "version": ">=4.0.0",
        "description": "TypeScript support required"
      }
    ]
  },
  "capabilities": {
    "installableComponents": [
      "button", "input", "card", "dialog", "form", "table", "badge", "avatar",
      "dropdown-menu", "sonner", "sheet", "tabs", "accordion", "carousel",
      "calendar", "date-picker", "alert-dialog", "alert", "checkbox", "switch",
      "collapsible", "context-menu", "hover-card", "menubar", "navigation-menu",
      "popover", "progress", "radio-group", "scroll-area", "slider", "toggle",
      "toggle-group", "label"
    ],
    "ui-components": {
      "version": "1.0.0",
      "description": "Beautifully designed React components",
      "provides": ["components", "radix-ui", "accessibility", "responsive", "typescript", "production-ready"]
    },
    "styling-system": {
      "version": "1.0.0",
      "description": "Complete styling and theming system",
      "provides": ["tailwind", "dark-mode", "theming", "theme-switching", "animations", "icons"]
    },
    "form-integration": {
      "version": "1.0.0",
      "description": "Form components and validation integration",
      "provides": ["form-validation", "form-components", "input-components"]
    },
    "utilities": {
      "version": "1.0.0",
      "description": "Comprehensive utility functions and helpers",
      "provides": ["comprehensive-utils", "class-utilities", "style-utilities"]
    }
  },
  "limitations": "Requires React framework and Tailwind CSS",
  "parameters": {
    "theme": {
      "type": "string",
      "default": "default",
      "description": "UI theme variant",
      "required": false,
      "options": ["default", "dark", "light", "minimal"]
    },
    "components": {
      "type": "array",
      "default": [
        "button",
        "input",
        "card",
        "dialog",
        "form",
        "table",
        "badge",
        "avatar",
        "dropdown-menu",
        "sonner",
        "sheet",
        "tabs",
        "accordion",
        "carousel",
        "calendar",
        "date-picker",
        "alert-dialog",
        "checkbox",
        "collapsible",
        "context-menu",
        "hover-card",
        "menubar",
        "navigation-menu",
        "popover",
        "progress",
        "radio-group",
        "scroll-area",
        "slider",
        "toggle",
        "toggle-group"
      ],
      "description": "Components to install (comprehensive set by default)",
      "required": false,
      "items": {
        "type": "string",
        "enum": [
          "alert",
          "alert-dialog",
          "accordion",
          "avatar",
          "badge",
          "button",
          "calendar",
          "card",
          "carousel",
          "checkbox",
          "collapsible",
          "context-menu",
          "date-picker",
          "dialog",
          "dropdown-menu",
          "form",
          "hover-card",
          "input",
          "label",
          "menubar",
          "navigation-menu",
          "pagination",
          "popover",
          "progress",
          "radio-group",
          "scroll-area",
          "separator",
          "sheet",
          "slider",
          "sonner",
          "switch",
          "table",
          "tabs",
          "textarea",
          "toggle",
          "toggle-group"
        ]
      }
    }
  },
  "features": {
    "theming": {
      "id": "theming",
      "name": "Theming Dependencies",
      "description": "Install theming dependencies (next-themes)",
      "blueprint": "features/theming.blueprint.ts",
      "category": "premium",
      "parameters": {}
    },
    "accessibility": {
      "id": "accessibility",
      "name": "Accessibility Dependencies",
      "description": "Install accessibility dependencies (eslint-plugin-jsx-a11y)",
      "blueprint": "features/accessibility.blueprint.ts",
      "category": "premium",
      "parameters": {}
    }
  },
  "constraints": {}
}