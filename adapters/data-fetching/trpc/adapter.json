{
  "id": "data-fetching/trpc",
  "name": "tRPC Client",
  "description": "End-to-end typesafe APIs with tRPC (wraps TanStack Query)",
  "type": "adapter",
  "version": "1.0.0",
  "category": "data-fetching",
  "requires": ["data-fetching/tanstack-query"],
  "provides": ["trpc-client", "trpc-react-hooks"],
  "parameters": {
    "transformer": {
      "type": "string",
      "enum": ["superjson", "devalue", "none"],
      "default": "superjson",
      "description": "Data transformer for complex types (Date, Map, Set)"
    },
    "abortOnUnmount": {
      "type": "boolean",
      "default": true,
      "description": "Abort requests on component unmount"
    },
    "batchingEnabled": {
      "type": "boolean",
      "default": true,
      "description": "Enable request batching for better performance"
    }
  }
}


