{
  "id": "technologies/analytics/ab-testing-core",
  "name": "A/B Testing Core (Tech-Agnostic)",
  "description": "Tech-agnostic A/B testing types, utilities, and experiment management. Framework-specific implementations handled by Connectors.",
  "category": "analytics",
  "version": "1.0.0",
  "role": "adapter",
  "blueprint": "blueprint.ts",
  "dependencies": [],
  "provides": [
    "ab-testing-core",
    "ab-testing-types",
    "ab-testing-utilities",
    "ab-testing-experiment-management"
  ],
  "limitations": "Tech-agnostic core only. Requires Connector for framework-specific implementation.",
  "parameters": {
    "features": {
      "core": {
        "type": "boolean",
        "default": true,
        "description": "Essential A/B testing utilities and types"
      },
      "experimentManagement": {
        "type": "boolean",
        "default": true,
        "description": "Experiment configuration and management"
      },
      "variantAssignment": {
        "type": "boolean",
        "default": true,
        "description": "Variant assignment logic"
      },
      "analytics": {
        "type": "boolean",
        "default": false,
        "description": "Analytics integration for tracking experiment results"
      }
    }
  },
  "internal_structure": {
    "core": {
      "provides": [
        "ab-testing-core"
      ],
      "templates": [
        "config.ts",
        "types.ts",
        "utils.ts"
      ]
    },
    "experimentManagement": {
      "prerequisites": [
        "ab-testing-core"
      ],
      "provides": [
        "ab-testing-experiment-management"
      ],
      "templates": [
        "experiments.ts"
      ]
    },
    "variantAssignment": {
      "prerequisites": [
        "ab-testing-core"
      ],
      "provides": [
        "ab-testing-variant-assignment"
      ],
      "templates": [
        "variant-assignment.ts"
      ]
    }
  },
  "constraints": {}
}






























