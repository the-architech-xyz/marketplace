{
  "id": "nextjs",
  "name": "Next.js",
  "description": "The React Framework for Production",
  "category": "framework",
  "version": "15.0.0",
  "blueprint": "blueprint.ts",
  "dependencies": [],
  "provides": [
    {
      "name": "react",
      "version": "19.1.0",
      "description": "React library for building user interfaces"
    },
    {
      "name": "nextjs",
      "version": "15.0.0",
      "description": "Next.js React framework"
    },
    {
      "name": "typescript",
      "version": "5.0.0",
      "description": "TypeScript language support"
    },
    {
      "name": "tailwind",
      "version": "4.1.0",
      "description": "Tailwind CSS v4 utility framework with shadcn/ui compatibility"
    },
    {
      "name": "eslint",
      "version": "8.0.0",
      "description": "ESLint code linting"
    }
  ],
  "capabilities": {
    "react-framework": {
      "version": "15.0.0",
      "description": "Complete React framework with Next.js",
      "provides": ["react", "nextjs", "app-router", "pages-router", "typescript"]
    },
    "rendering-modes": {
      "version": "1.0.0",
      "description": "Server-side and static rendering capabilities",
      "provides": ["ssr", "ssg", "api-routes"]
    },
    "styling-system": {
      "version": "1.0.0",
      "description": "Styling and CSS framework integration",
      "provides": ["tailwind", "css-modules", "styled-components"]
    },
    "development-tools": {
      "version": "1.0.0",
      "description": "Development and build tools",
      "provides": ["eslint", "typescript", "src-dir", "import-alias", "modern-dev-tools"]
    },
    "performance-optimization": {
      "version": "1.0.0",
      "description": "Performance and optimization features",
      "provides": ["performance-optimization", "bundle-analysis", "image-optimization", "seo-optimization"]
    },
    "security": {
      "version": "1.0.0",
      "description": "Security headers and features",
      "provides": ["security-headers", "csp", "cors"]
    }
  },
  "limitations": "Requires Node.js 18+ and npm/yarn/pnpm/bun",
  "paths": {
    "source_root": "src/",
    "app_root": "src/app/",
    "pages_root": "src/pages/",
    "shared_library": "src/lib/",
    "utils": "src/lib/utils/",
    "types": "src/types/",
    "hooks": "src/hooks/",
    "stores": "src/stores/",
    "components": "src/components/",
    "ui_components": "src/components/ui/",
    "layouts": "src/components/layouts/",
    "providers": "src/components/providers/",
    "api_routes": "src/app/api/",
    "api_handlers": "src/app/api/",
    "middleware": "src/middleware/",
    "config": "src/config/",
    "env": ".env",
    "scripts": "scripts/",
    "database_config": "src/lib/db/",
    "auth_config": "src/lib/auth/",
    "payment_config": "src/lib/payment/",
    "email_config": "src/lib/email/",
    "observability_config": "src/lib/observability/",
    "state_config": "src/lib/state/",
    "testing_config": "src/lib/testing/",
    "deployment_config": "src/lib/deployment/",
    "content_config": "src/lib/content/",
    "blockchain_config": "src/lib/blockchain/",
    "assets": "public/",
    "public": "public/",
    "static": "public/",
    "styles": "src/styles/",
    "tests": "__tests__/",
    "test_utils": "src/__tests__/utils/",
    "mocks": "src/__mocks__/",
    "build": ".next/",
    "dist": "dist/",
    "out": "out/",
    "docs": "docs/",
    "readme": "README.md"
  },
  "parameters": {
    "typescript": {
      "type": "boolean",
      "default": true,
      "description": "Enable TypeScript support",
      "required": false
    },
    "tailwind": {
      "type": "boolean",
      "default": true,
      "description": "Enable Tailwind CSS",
      "required": false
    },
    "eslint": {
      "type": "boolean",
      "default": true,
      "description": "Enable ESLint",
      "required": false
    },
    "appRouter": {
      "type": "boolean",
      "default": true,
      "description": "Use App Router (recommended)",
      "required": false
    },
    "srcDir": {
      "type": "boolean",
      "default": true,
      "description": "Use src/ directory",
      "required": false
    },
    "importAlias": {
      "type": "string",
      "default": "@/*",
      "description": "Import alias for absolute imports",
      "required": false
    },
    "features": {
      "seo": {
        "type": "boolean",
        "default": false,
        "description": "Enable SEO optimization features"
      },
      "imageOptimization": {
        "type": "boolean",
        "default": false,
        "description": "Enable Next.js image optimization"
      },
      "mdx": {
        "type": "boolean",
        "default": false,
        "description": "Enable MDX support for markdown content"
      },
      "performance": {
        "type": "boolean",
        "default": false,
        "description": "Enable performance optimization features"
      },
      "streaming": {
        "type": "boolean",
        "default": false,
        "description": "Enable streaming features"
      },
      "i18n": {
        "type": "boolean",
        "default": false,
        "description": "Enable internationalization features"
      }
    }
  },
  "features": {
    "api-routes": {
      "id": "api-routes",
      "name": "API Routes",
      "description": "Comprehensive API routes with middleware and error handling",
      "blueprint": "features/api-routes.blueprint.ts",
      "category": "core",
      "parameters": {
        "middleware": {
          "type": "boolean",
          "default": true,
          "description": "Include API middleware",
          "required": false
        },
        "error-handling": {
          "type": "boolean",
          "default": true,
          "description": "Include error handling utilities",
          "required": false
        }
      }
    },
    "performance": {
      "id": "performance",
      "name": "Performance Optimization",
      "description": "Advanced performance optimization tools for Next.js 15+",
      "blueprint": "features/performance.blueprint.ts",
      "category": "premium",
      "parameters": {
        "bundle-analysis": {
          "type": "boolean",
          "default": true,
          "description": "Include bundle analysis tools",
          "required": false
        },
        "web-vitals": {
          "type": "boolean",
          "default": true,
          "description": "Enable Web Vitals monitoring",
          "required": false
        },
        "lighthouse": {
          "type": "boolean",
          "default": true,
          "description": "Include Lighthouse performance testing",
          "required": false
        }
      }
    },
    "security": {
      "id": "security",
      "name": "Security Headers",
      "description": "Comprehensive security middleware and headers for Next.js 15+",
      "blueprint": "features/security.blueprint.ts",
      "category": "premium",
      "parameters": {
        "csp": {
          "type": "boolean",
          "default": true,
          "description": "Enable Content Security Policy",
          "required": false
        },
        "rate-limiting": {
          "type": "boolean",
          "default": true,
          "description": "Enable rate limiting",
          "required": false
        },
        "bot-protection": {
          "type": "boolean",
          "default": true,
          "description": "Enable bot detection and protection",
          "required": false
        }
      }
    },
    "server-actions": {
      "id": "server-actions",
      "name": "Server Actions",
      "description": "Modern Server Actions implementation for Next.js 15+",
      "blueprint": "features/server-actions.blueprint.ts",
      "category": "core",
      "parameters": {
        "examples": {
          "type": "boolean",
          "default": true,
          "description": "Include example server actions",
          "required": false
        },
        "validation": {
          "type": "boolean",
          "default": true,
          "description": "Include form validation utilities",
          "required": false
        },
        "rate-limiting": {
          "type": "boolean",
          "default": true,
          "description": "Enable rate limiting for server actions",
          "required": false
        }
      }
    },
    "middleware": {
      "id": "middleware",
      "name": "Middleware",
      "description": "Next.js middleware for auth, redirects, and more",
      "blueprint": "features/middleware.blueprint.ts",
      "category": "premium",
      "parameters": {
        "auth": {
          "type": "boolean",
          "default": true,
          "description": "Include auth middleware",
          "required": false
        },
        "redirects": {
          "type": "boolean",
          "default": true,
          "description": "Include redirect middleware",
          "required": false
        }
      }
    },
    "seo": {
      "id": "seo",
      "name": "SEO Optimization",
      "description": "Next.js SEO tools with next-seo and sitemap generation",
      "blueprint": "features/seo.blueprint.ts",
      "category": "premium",
      "parameters": {
        "next-seo": {
          "type": "boolean",
          "default": true,
          "description": "Enable next-seo for meta tags",
          "required": false
        },
        "sitemap": {
          "type": "boolean",
          "default": true,
          "description": "Enable automatic sitemap generation",
          "required": false
        }
      }
    }
  },
  "constraints": {}
}