{
  "id": "team-billing",
  "name": "Team Billing",
  "description": "Billing and subscription management for teams",
  "version": "1.0.0",
  "category": "sub-feature",
  "parent_feature": "teams-management",
  "prerequisites": {
    "adapters": ["stripe"],
    "integrators": ["stripe-nextjs-integration"],
    "capabilities": ["payment-processing", "subscription-management"]
  },
  "provides": {
    "capabilities": [
      "team-subscription-management",
      "billing-dashboard",
      "payment-methods",
      "invoice-management"
    ]
  },
  "parameters": {
    "billingModel": {
      "type": "select",
      "default": "per-member",
      "description": "Billing model for teams",
      "required": false,
      "options": ["per-member", "flat-rate", "usage-based"]
    },
    "trialPeriod": {
      "type": "number",
      "default": 14,
      "description": "Trial period in days",
      "required": false
    }
  }
}
