{
  "id": "auth-dashboard-backend-nextjs",
  "name": "Auth Dashboard Backend (Next.js)",
  "description": "Backend implementation of auth-dashboard using Next.js API routes and Better Auth",
  "version": "1.0.0",
  "actions": [
    {
      "type": "CREATE_FILE",
      "path": "src/hooks/auth/use-auth.ts",
      "template": "templates/use-auth.ts.tpl"
    },
    {
      "type": "CREATE_FILE",
      "path": "src/hooks/auth/use-sign-in.ts",
      "template": "templates/use-sign-in.ts.tpl"
    },
    {
      "type": "CREATE_FILE",
      "path": "src/hooks/auth/use-sign-up.ts",
      "template": "templates/use-sign-up.ts.tpl"
    },
    {
      "type": "CREATE_FILE",
      "path": "src/hooks/auth/use-sign-out.ts",
      "template": "templates/use-sign-out.ts.tpl"
    },
    {
      "type": "CREATE_FILE",
      "path": "src/hooks/auth/use-user.ts",
      "template": "templates/use-user.ts.tpl"
    },
    {
      "type": "CREATE_FILE",
      "path": "src/hooks/auth/use-session.ts",
      "template": "templates/use-session.ts.tpl"
    },
    {
      "type": "CREATE_FILE",
      "path": "src/types/auth.ts",
      "template": "templates/auth-types.ts.tpl"
    }
  ]
}
