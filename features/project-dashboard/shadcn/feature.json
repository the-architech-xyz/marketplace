{
  "id": "features/project-dashboard/shadcn",
  "name": "Project Dashboard",
  "description": "Main SaaS dashboard for managing projects and viewing analytics",
  "version": "1.0.0",
  "category": "feature",
  "tags": ["dashboard", "projects", "analytics", "shadcn", "saas"],
  "compatibility": {
    "frameworks": ["nextjs"],
    "adapters": ["ui/shadcn-ui", "database/drizzle"],
    "integrations": ["drizzle-nextjs-integration", "tanstack-query-nextjs-integration"]
  },
  "dependencies": {
    "adapters": ["ui/shadcn-ui", "database/drizzle"],
    "integrations": ["drizzle-nextjs-integration", "tanstack-query-nextjs-integration"]
  },
  "features": {
    "projectList": true,
    "projectCards": true,
    "projectCreation": true,
    "projectAnalytics": true,
    "recentActivity": true,
    "quickActions": true,
    "searchAndFilter": true,
    "pagination": true
  },
  "parameters": {
    "components": {
      "type": "array",
      "description": "Dashboard components to include",
      "default": ["project-list", "project-cards", "analytics", "activity"],
      "enum": ["project-list", "project-cards", "analytics", "activity", "quick-actions", "search", "filters"]
    },
    "layout": {
      "type": "object",
      "description": "Dashboard layout configuration",
      "properties": {
        "sidebar": {
          "type": "boolean",
          "default": true
        },
        "header": {
          "type": "boolean",
          "default": true
        },
        "grid": {
          "type": "string",
          "default": "responsive",
          "enum": ["responsive", "fixed", "masonry"]
        }
      }
    },
    "analytics": {
      "type": "object",
      "description": "Analytics configuration",
      "properties": {
        "enabled": {
          "type": "boolean",
          "default": true
        },
        "metrics": {
          "type": "array",
          "default": ["total-projects", "active-projects", "recent-activity"],
          "enum": ["total-projects", "active-projects", "recent-activity", "user-growth", "performance"]
        }
      }
    }
  },
  "blueprint": {
    "file": "blueprint.ts"
  }
}
