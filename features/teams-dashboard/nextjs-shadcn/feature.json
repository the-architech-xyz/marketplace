{
  "id": "teams-dashboard-nextjs-shadcn",
  "name": "Teams Dashboard (Next.js + Shadcn)",
  "description": "Complete teams management dashboard with Next.js and Shadcn/ui",
  "version": "1.0.0",
  "category": "feature",
  "blueprint": "blueprint.ts",
  "prerequisites": {
    "adapters": [
      "nextjs",
      "shadcn-ui",
      "drizzle",
      "better-auth"
    ],
    "integrators": [
      "drizzle-nextjs",
      "better-auth-nextjs",
      "teams-data-integration"
    ]
  },
  "capabilities": [
    "teams-management",
    "user-dashboard",
    "team-collaboration",
    "team-settings",
    "team-members",
    "team-invitations"
  ],
  "parameters": {
    "theme": {
      "type": "string",
      "default": "default",
      "description": "UI theme variant",
      "required": false,
      "options": ["default", "dark", "light"]
    },
    "features": {
      "type": "object",
      "default": {
        "analytics": true,
        "goals": true,
        "permissions": true,
        "invitations": true
      },
      "description": "Teams dashboard features to enable",
      "required": false
    }
  },
  "constraints": {
    "minReactVersion": ">=18.0.0",
    "minNextjsVersion": ">=13.0.0"
  }
}
