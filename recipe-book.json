{
  "version": "1.0.0",
  "packages": {
    "auth": {
      "defaultProvider": "better-auth",
      "packageStructure": {
        "name": "auth",
        "directory": "packages/auth",
        "dependencies": {
          "better-auth": "latest",
          "@{projectName}/db": "workspace:*"
        },
        "devDependencies": {
          "typescript": "^5.0.0",
          "@types/node": "^20.0.0"
        },
        "exports": {
          "./client": "./src/client.ts",
          "./server": "./src/server.ts",
          "./types": "./src/types.ts"
        }
      },
      "providers": {
        "better-auth": {
          "modules": [
            {
              "id": "adapters/auth/better-auth",
              "version": "1.0.0",
              "targetPackage": "auth"
            },
            {
              "id": "connectors/auth/better-auth-nextjs",
              "version": "1.0.0",
              "targetPackage": null,
              "targetApps": ["web", "api"],
              "requiredFramework": "nextjs"
            },
            {
              "id": "features/auth/frontend",
              "version": "1.0.0",
              "targetPackage": null,
              "targetApps": ["web", "mobile"]
            },
            {
              "id": "features/auth/tech-stack",
              "version": "1.0.0",
              "targetPackage": "auth"
            }
          ],
          "dependencies": {
            "packages": [
              "ui",
              "database"
            ]
          }
        }
      }
    },
    "ui": {
      "defaultProvider": "tamagui",
      "packageStructure": {
        "name": "ui",
        "directory": "packages/ui",
        "dependencies": {
          "@tamagui/core": "latest",
          "@tamagui/config": "latest",
          "react-native-web": "latest"
        },
        "devDependencies": {
          "typescript": "^5.0.0",
          "@types/node": "^20.0.0",
          "@types/react": "^18.0.0",
          "@types/react-native": "^0.73.0"
        },
        "exports": {
          "./components": "./src/components/index.ts",
          "./themes": "./src/themes/index.ts",
          "./utils": "./src/utils/index.ts"
        }
      },
      "providers": {
        "tamagui": {
          "modules": [
            {
              "id": "adapters/ui/tamagui",
              "version": "1.0.0",
              "targetPackage": "ui"
            },
            {
              "id": "connectors/ui/tamagui-nextjs",
              "version": "1.0.0",
              "targetPackage": null,
              "targetApps": ["web"],
              "requiredFramework": "nextjs",
              "requiredAppTypes": ["web"]
            },
            {
              "id": "connectors/ui/tamagui-expo",
              "version": "1.0.0",
              "targetPackage": null,
              "targetApps": ["mobile"],
              "requiredFramework": "expo",
              "requiredAppTypes": ["mobile"]
            }
          ],
          "dependencies": {
            "packages": []
          }
        },
        "shadcn": {
          "modules": [
            {
              "id": "adapters/ui/shadcn-ui",
              "version": "1.0.0",
              "targetPackage": "ui"
            }
          ],
          "dependencies": {
            "packages": []
          }
        }
      }
    },
    "database": {
      "defaultProvider": "drizzle",
      "packageStructure": {
        "name": "db",
        "directory": "packages/db",
        "dependencies": {
          "drizzle-orm": "latest",
          "postgres": "latest"
        },
        "devDependencies": {
          "typescript": "^5.0.0",
          "@types/node": "^20.0.0",
          "drizzle-kit": "latest"
