{
  "name": "@thearchitech.xyz/marketplace",
  "version": "0.9.3",
  "description": "The Architech Marketplace - Adapters, Connectors, Capabilities, and Features",
  "type": "module",
  "main": "index.js",
  "exports": {
    ".": "./index.js",
    "./types": "./dist/types/index.js",
    "./adapters/*": "./adapters/*/adapter.json",
    "./connectors/*": "./connectors/*/connector.json",
    "./capabilities/*": "./capabilities/*/capability.json",
    "./features/*": "./features/*/feature.json",
    "./genomes/*": "./genomes/*.genome.ts",
    "./genomes": "./genomes/manifest.json",
    "./manifest": "./manifest.json",
    "./adapter": "./adapter/index.js"
  },
  "types": "./index.d.ts",
  "files": [
    "adapters/**/*.json",
    "adapters/**/templates/",
    "adapters/**/features/",
    "connectors/**/*.json",
    "connectors/**/templates/",
    "capabilities/**/*.json",
    "capabilities/**/templates/",
    "features/**/*.json",
    "features/**/templates/",
    "dist/adapters/**/*.js",
    "dist/adapters/**/*.d.ts",
    "dist/connectors/**/*.js",
    "dist/connectors/**/*.d.ts",
    "dist/capabilities/**/*.js",
    "dist/capabilities/**/*.d.ts",
    "dist/features/**/*.js",
    "dist/features/**/*.d.ts",
    "genomes/*.genome.ts",
    "genomes/manifest.json",
    "manifest.json",
    "index.js",
    "index.d.ts",
    "types/",
    "adapter/"
  ],
  "keywords": [
    "architech",
    "marketplace",
    "adapters",
    "connectors",
    "capabilities",
    "features",
    "blueprints"
  ],
  "author": "The Architech Team",
  "license": "AGPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/thearchitech-xyz/marketplace.git"
  },
  "bugs": {
    "url": "https://github.com/thearchitech-xyz/marketplace/issues"
  },
  "homepage": "https://thearchitech.xyz",
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "validate": "tsx scripts/cli.ts validate --blueprints",
    "validate:templates": "tsx scripts/cli.ts validate --templates",
    "validate:conflicts": "tsx scripts/cli.ts validate --conflicts",
    "validate:contracts": "tsx scripts/cli.ts validate --contracts",
    "validate:comprehensive": "tsx scripts/cli.ts validate --comprehensive",
    "validate:dead-templates": "tsx scripts/cli.ts validate --dead-templates",
    "validate:actions": "tsx scripts/cli.ts validate --actions",
    "validate:issues": "tsx scripts/cli.ts validate --issues",
    "validate:correctness": "tsx scripts/cli.ts validate --contracts",
    "validate:correctness:changed": "tsx scripts/cli.ts validate --contracts",
    "generate:manifest": "tsx scripts/cli.ts generate --manifest",
    "generate:capability-manifest": "tsx scripts/cli.ts generate --capability-first",
    "generate:feature-manifests": "tsx scripts/cli.ts generate --feature-manifests",
    "types:generate:constitutional": "tsx scripts/cli.ts generate --types",
    "validate:all": "tsx scripts/cli.ts validate --all",
    "validate:schema": "tsx scripts/cli.ts validate --module-schema",
    "validate:legacy": "tsx scripts/cli.ts validate --all",
    "typecheck": "tsc --noEmit",
    "check": "tsx scripts/cli.ts build --check",
    "build:dist": "tsx scripts/cli.ts build --build",
    "build": "tsx scripts/cli.ts build --all",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "prepare": "husky install",
    "precommit": "npm run validate:correctness"
  },
  "dependencies": {
    "@thearchitech.xyz/types": "^0.9.2",
    "@thearchitech.xyz/genome-transformer": "^1.1.1",
    "@types/glob": "^9.0.0",
    "jscodeshift": "^17.3.0"
  },
  "devDependencies": {
    "@types/node": "^24.6.1",
    "glob": "^10.4.5",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "ts-morph": "^23.0.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "zod": "^4.1.12"
  },
  "lint-staged": {
    "adapters/**/*.json": "node scripts/lint-staged-wrapper.js",
    "integrations/**/*.json": "node scripts/lint-staged-wrapper.js",
    "connectors/**/*.json": "node scripts/lint-staged-wrapper.js",
    "features/**/*.json": "node scripts/lint-staged-wrapper.js",
    "scripts/generate-types.ts": "node scripts/lint-staged-wrapper.js",
    "scripts/generate-constitutional-types.ts": "node scripts/lint-staged-wrapper.js"
  }
}
