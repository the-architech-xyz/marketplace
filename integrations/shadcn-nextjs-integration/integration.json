{
  "id": "shadcn-nextjs-integration",
  "name": "Shadcn Next.js Integration",
  "description": "Integrates Shadcn/ui with Next.js by configuring Tailwind and providing theme providers",
  "category": "integration",
  "version": "1.0.0",
  "prerequisites": {
    "capabilities": [
      {
        "name": "nextjs",
        "version": ">=15.0.0",
        "description": "Next.js framework required"
      },
      {
        "name": "tailwind",
        "version": ">=3.0.0",
        "description": "Tailwind CSS required"
      },
      {
        "name": "shadcn-ui",
        "version": ">=2.0.0",
        "description": "Shadcn/ui components required"
      }
    ]
  },
  "provides": [
    {
      "name": "shadcn-nextjs-integration",
      "version": "1.0.0",
      "description": "Shadcn/ui integration for Next.js"
    }
  ],
  "parameters": {
    "theming": {
      "type": "boolean",
      "default": false,
      "description": "Enable theming integration (requires next-themes)",
      "required": false
    }
  }
}
