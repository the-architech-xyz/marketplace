{
  "id": "teams-dashboard-shadcn",
  "name": "Teams Dashboard Shadcn",
  "description": "Teams dashboard UI components with Shadcn/ui - uses headless teams logic",
  "version": "1.0.0",
  "category": "ui",
  "requirements": {
    "capabilities": ["shadcn-ui", "teams-dashboard-core"]
  },
  "capabilities": [
    "teams-ui",
    "shadcn-components",
    "teams-dashboard",
    "team-management-ui",
    "team-analytics-ui"
  ],
  "limitations": "Requires teams-dashboard-core for functionality",
  "parameters": {
    "components": {
      "type": "array",
      "default": [
        "TeamsDashboard",
        "TeamsList",
        "TeamCard",
        "CreateTeamForm",
        "TeamSettings",
        "TeamMembers",
        "TeamInvitations",
        "TeamAnalytics",
        "TeamGoals",
        "TeamActivity"
      ],
      "description": "Teams UI components to install",
      "required": false
    }
  },
  "constraints": {}
}
